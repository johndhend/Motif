! This is the applications defaults file for the Performance Container directory

!*****************************************************************************!
!			Defaults for PerfCntnr1.c                             !
!*****************************************************************************!

PerfCntnr1.width:300
PerfCntnr1.height:300

PerfCntnr1*ScrolledW.width:300
PerfCntnr1*ScrolledW.height:300

PerfCntnr1*largeIconPixmap:		../../Toolkit/bitmaps/LargeIcon
!PerfCntnr1*smallIconPixmap:		../../Toolkit/bitmaps/SmallIcon
PerfCntnr1*highlightColor:		red

PerfCntnr1*outlineState:		expanded

! There are few sets of Container resources for the test. Which one is applied
! depends on the flag the user passes into the test, which determines the
! Xt name of the Container.  

PerfCntnr1*entryViewType:		large_icon
PerfCntnr1*layoutType:			outline

!*****************************************************************************!
!			Defaults for PerfCntnr2.c                             !
!*****************************************************************************!


PerfCntnr2*largeIconPixmap:		../../Toolkit/bitmaps/LargeIcon
!PerfCntnr2*smallIconPixmap:		../../Toolkit/bitmaps/SmallIcon
PerfCntnr2*highlightColor:		red

PerfCntnr2*outlineState:		expanded

! There are few sets of Container resources for the test. Which one is applied
! depends on the flag the user passes into the test, which determines the
! Xt name of the Container.  

PerfCntnr2*layoutType:			outline

