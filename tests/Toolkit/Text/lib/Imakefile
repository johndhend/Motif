#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = Auto Manual 

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))

all::   libTextA.a libTextM.a

libTextA.a:
	$(RM) libTextA.a
	$(LN) Auto/libText.a libTextA.a

libTextM.a:
	$(RM) libTextM.a
	$(LN) Manual/libText.a libTextM.a

clean::
	$(RM) libTextA.a libTextM.a

XCOMM	During make includes make Makefile in Auto for Automation.

includes::
	echo "making Makefile in $(CURRENT_DIR)/Auto with Automation"
	@(set -x; cd Auto; make IMAKE_DEFINES=-DAUTOMATION Makefile)

