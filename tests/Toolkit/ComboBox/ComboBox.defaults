/* Defaults file for test ComboBox1.c */

! defaults when run w/ flag -u 1

ComboBox1*CB1.comboBoxType: 		COMBO_BOX
ComboBox1*CB1.fontList : 		8x13bold
ComboBox1*CB1.matchBehavior : 		NONE
ComboBox1*CB1.layoutDirection : 	LEFT_TO_RIGHT
ComboBox1*CB1.highlightThickness : 	10
ComboBox1*CB1.marginWidth : 		12
ComboBox1*CB1.marginHeight : 		12
ComboBox1*CB1.selectedPosition : 	2

! defaults when run w/ flag -u 2

ComboBox1*CB2.comboBoxType:             DROP_DOWN_COMBO_BOX
ComboBox1*CB2.layoutDirection : 	RIGHT_TO_LEFT
ComboBox1*CB2.arrowSize : 		15
ComboBox1*CB2.arrowSpacing : 		10


! defaults when run w/ flag -u 3

ComboBox1*CB3.comboBoxType: 	        DROP_DOWN_LIST
ComboBox1*CB3.matchBehavior : 		QUICK_NAVIGATE


!set items for ComboBox -- applicable for all tests

ComboBox1*items :       grass,trees,leaves,dew,mountains,sunrise
ComboBox1*itemCount :   6
ComboBox1*visibleItemCount :   6




!Defaults for ComboBox2


ComboBox2*ComboBox*items : broccoli,cauliflower,carrots,potatoes,zucchini,squash,okra,onions,peppers,beans,yam,lettuce,cabbage,olives,pimento,cucumber,tomatoes, chestnuts, radish, snowpeas
ComboBox2*ComboBox*itemCount : 20
ComboBox2*ComboBox*visibleItemCount : 8
ComboBox2*ComboBox.selectedItem : carrots



