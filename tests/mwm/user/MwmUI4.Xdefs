! HISTORY
! defaults file for MwmUI4.c

!**************************************************************************!
!Coloring Resources
!**************************************************************************!

! Windows, icons, menus

Mwm*activeBackground : IndianRed
Mwm*activeForeground : wheat
Mwm*background :OldLace
Mwm*foreground :IndianRed
Mwm*menu*TestMenu*background : SteelBlue
Mwm*menu*TestMenu*foreground : black


! mattes

! default values


!Icon Images

Mwm*iconImageBackground :oldlace
Mwm*iconImageBottomShadowColor :LightPink
Mwm*iconImageForeground :lavender
Mwm*iconImageTopShadowColor :black


!**************************************************************************!
! Window Decorations
!**************************************************************************!

Mwm*clientDecorations : all
Mwm*TestWindow1*clientFunctions : -maximize -move
Mwm*transientDecorations : menu title resizeh
Mwm*transientFunctions : -resize 



!*************************************************************************!
! Focus Policies
!*************************************************************************!


Mwm*keyboardFocusPolicy :explicit
Mwm*deiconifyKeyFocus : True
Mwm*focusAutoRaise : False


!*************************************************************************!
! Controlling Windows
!*************************************************************************!

! font

Mwm*fontList : 8x13bold

! Button Bindings

Mwm*buttonBindings : MwmUI4ButtonBindings
Mwm*keyBindings    : MwmUI4KeyBindings

! Config file

Mwm*configFile : MwmUI4.mwmrc

! Window Menus

Mwm*windowMenu : MwmUI4WindowMenu
Mwm*wMenuButtonClick2 : False

!**************************************************************************!
!Controlling Window Size and Position
!**************************************************************************!

! Size Resources


Mwm*resizeCursors : False
Mwm*maximumMaximumSize : 500x500

! Position Resources

Mwm*clientAutoPlace : True
Mwm*showFeedback : -move




!**************************************************************************!
!Configuring icons
!**************************************************************************!

Mwm*iconPlacement : top right
Mwm*iconPlacementMargin :50
Mwm*lowerOnIconify :True
Mwm*iconDecoration : image label
Mwm*iconImage : wingdogs


!**************************************************************************!
!Configuring the icon box
!**************************************************************************!



Mwm*useIconBox :False




! End of mwm defaults file


























