!
!  General Appearance
!
*blinkRate:			0

*cursorColor:			black
*pointerColor:			black
!
! New for R5 default font:
!
XtDefaultFont:			fixed
!
!  Motif Toolkit
!
*XmScrollBar*repeatDelay:	32767
*XmScrollBar*initialDelay:	32767

*XmPanedWindow*allowResize:	True
!
!  Motif Window Manager
!
Mwm*fontList:			9x15
Mwm*icon*fontList:		9x15

Mwm*keyboardFocusPolicy:	pointer
Mwm*focusAutoRaise:		False
Mwm*clientAutoPlace:		False

Mwm*background:			steelblue
Mwm*foreground:			wheat
Mwm*menu*background:        	cyan
Mwm*menu*foreground:        	black

Mwm*rootMenu : 			DefaultRootMenu

Mwm*useIconBox:			False
Mwm*resizeBorderWidth:		6
!
!  xterm
!
XTerm*iconName:			xterm
XTerm*font:			9x15
XTerm*scrollBar:                true
XTerm*saveLines:                1024
XTerm*background:		wheat
XTerm*foreground:		black
